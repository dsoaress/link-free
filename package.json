{
  "name": "link-free",
  "private": true,
  "description": "An open source alternative to Linktree built by Mars with speed and customization in mind.",
  "author": "Mars Collective <hi@marscollective.co>",
  "version": "2.0.0",
  "dependencies": {
    "gatsby": "^2.27.1",
    "gatsby-plugin-google-tagmanager": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.0",
    "gatsby-plugin-netlify-cms": "^4.4.0",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "netlify-cms-app": "^2.13.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "2.2.0"
  },
  "license": "0BSD",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "yarn build && netlify deploy -p",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marscollective/link-free"
  },
  "bugs": {
    "url": "https://github.com/marscollective/link-free/issues"
  },
  "engines": {
    "node": "12.x"
